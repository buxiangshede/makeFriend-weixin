.container {
  position: fixed;
  top: 0;
  left:0;
  width: 100%;
  height: 120rpx;
  z-index: 9999;
  overflow: hidden;

}
.containerFocus {
  position: fixed;
  top: 0;
  left:0;
  width: 100%;
  height: 100%;
  z-index: 9999;
  background-color: #ccc;
}
.search{
  display: flex;
  align-items: center;
  margin: 20rpx;
}
.search-text{
  display: flex;
  flex:1;
  align-items: center;
  margin: 20rpx;
  border: 1px solid #b4b5b6;
  border-radius: 10rpx;
  height: 60rpx;
  background-color: white;
}
.search-text input {flex: 1}
.icon-search {
  margin: 0 10rpx;
}
.search-cancel {
  margin: 0 10rpx;
}
.search-history {
  display: flex;
  justify-content: space-between;
  margin:  20rpx;
  margin-bottom: 30rpx;
}
.search-history-btn {
  margin-bottom: 30rpx;
}
.search-history-btn text{
  border-radius: 10rpx;
  margin: 10rpx;
  background-color: #b4b5b6;
  border:  1px solid #b4b5b6;
  padding:  10rpx 20rpx;
  background-color: white; 
}

.search-list-item {
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx;
  border-bottom: 1px dashed #b4b5b6;
}
.search-list-item view{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.search-list-item image{
  height: 100rpx;
  width: 100rpx;
  border-radius: 50%;
}
.search-list-gap {
  margin-left: 20rpx;
}